<div class="row">
    <aside class="col-md-4">
      <section class="user_info">
        <h1 id="myname">
          <%= gravatar_for @user %>
          <%= @user.name %>
        </h1>
      </section>
    </aside>
    <div class="col-md-8">
      <section  class="followings_followers">
          <h2 class="panel-heading">Followers</h2>
            <ul>
              <%= render @users %>
            </ul>
      </section>
    </div>
</div>